<?xml version="1.0" encoding="UTF-8"?>


<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>TeamCity.SharedResources</name>
    <display-name>Shared Resources</display-name>
    <version>@Plugin_Version@</version> <!-- will be replaced from script -->
    <description>Allows defining resources and limiting concurrently running builds that use these resources </description>
    <vendor>
      <name>@Plugin_Vendor@</name>  <!-- will be replaced from script -->
      <url>@Plugin_Vendor_Url@</url> <!-- will be replaced from script -->
    </vendor>
  </info>
  <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
</teamcity-plugin>